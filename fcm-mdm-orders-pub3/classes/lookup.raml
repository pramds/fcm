        required: false
      externalId:
        type: external_id_type
        required: false
      billingFrequency:
        type: name_type
        required: false
  delete: 
    description: Invalidates the list of Billing Frequencies
    responses: 
      200:
        body: 
          application/json: 
            example: { "message": "Cache Invalidated" }  
