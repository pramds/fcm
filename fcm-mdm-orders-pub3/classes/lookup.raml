    description: External Id of the <<resourcePathName>>
    type: string
    required: false
  name_type:
    displayName: Name
    description: Name of the <<resourcePathName>>
    type: string
    required: false
  acct_num_type:
    displayName: Account Number
    description: Account Number of the <<resourcePathName>>
    type: string|integer
    required: false
  unspsc_code_type:
    displayName: Unspsc Code
    description: Unspsc Code of the <<resourcePathName>>
    type: number
    format: float
    required: false
  query_param_type:
    type: internal_id_type | external_id_type | name_type | acct_num_type | unspsc_code_type
    
traits:
  client-id-required:
    queryParameters:
      client_id:
        type: string
      client_secret:
        type: string
        
